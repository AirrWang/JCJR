<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 填充的颜色 -->
    <solid android:color="@color/white"/>
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners android:bottomRightRadius="@dimen/dimen_10px"
        android:topRightRadius="@dimen/dimen_10px"/>

    <padding
        android:bottom="1px"
        android:right="1px"
        android:top="1px"
        android:left="0px"/>
    <!-- 边框宽度，颜色值 -->
    <stroke
        android:width="1px"
        android:color="@color/btn_main"/>

</shape>